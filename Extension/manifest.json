{
  "manifest_version": 3,
  "name": "7tv Local",
  "description": "Save emotes",
  "version": "0.1",
  "permissions": ["storage", "tabs"],
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "js": ["cs.js"],
      "matches": ["https://www.twitch.tv/*"],
      "match_origin_as_fallback": false
    }
  ]
}